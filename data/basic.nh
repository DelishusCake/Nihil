// See basic.c for the output from this file
do_print :: (lim:u32)
{
	for (let var i:u32 = lim; i > 0; i--)
	{
		printf("%d ", i);
	}
	printf("\n");
}

main :: () -> i32
{
	do_print(100);

	// Basic pointer types!
	let var n:i32 = 1;
	let r:ptr<var i32> = &n;
	let p:ptr<ptr<var i32>> = &r;
	printf("\n%d %d\n", *r, **p);
	
	return 0;
}